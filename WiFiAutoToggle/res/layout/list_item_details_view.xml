<?xml version="1.0" encoding="utf-8"?>
<!-- version = "$Id$"; -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical">
	<TextView android:gravity="left" android:textSize="24sp"
		android:textStyle="bold" android:layout_height="wrap_content"
		android:text="@string/listItemDetailsViewLocation"
		android:layout_width="wrap_content" android:id="@+id/listItemDetailsViewLocationName"></TextView>
	<TableLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/listItemDetailsTable"
		android:stretchColumns="1">
		<TableRow>
			<TextView android:layout_column="1" android:padding="3dip"
				android:text="@string/listItemDetailsViewAdded" />
			<TextView android:gravity="right" android:padding="3dip"
				android:id="@+id/listItemDetailsViewAddedDateTime" />
		</TableRow>
		<TableRow>
			<TextView android:layout_column="1" android:padding="3dip"
				android:text="@string/listItemDetailsViewLatitude" />
			<TextView android:gravity="right" android:padding="3dip"
				android:id="@+id/listItemDetailsViewLatitude" />
		</TableRow>
		<TableRow>
			<TextView android:layout_column="1" android:padding="3dip"
				android:text="@string/listItemDetailsViewLongitude" />
			<TextView android:gravity="right" android:padding="3dip"
				android:id="@+id/listItemDetailsViewLongitude" />
		</TableRow>
		<TableRow>
			<TextView android:layout_column="1" android:padding="3dip"
				android:text="@string/listItemDetailsViewAccuracy" />
			<TextView android:gravity="right" android:padding="3dip"
				android:id="@+id/listItemDetailsViewAccuracy" />
		</TableRow>
	</TableLayout>
</LinearLayout>
